{
  "name": "training-system",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit",
    "db:seed": "tsx prisma/seed.ts",
    "scaffold:routes": "node scripts/make-tree.mjs",
    "scaffold:stubs": "node scripts/make-stubs.mjs",
    "test": "vitest run",
    "test:ui": "vitest",
    "test:api": "start-server-and-test start http://localhost:3000 'vitest run'",
