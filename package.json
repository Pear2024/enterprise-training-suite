    "postcss": "^8.5.6",
    "prisma": "^6.17.0",
    "start-server-and-test": "^2.1.2",
    "tailwindcss": "^4.1.14",
    "tailwindcss-animate": "^1.0.7",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.6",
    "typescript": "^5",
    "undici": "^7.16.0",
    "vitest": "^3.2.4",
    "wait-on": "^9.0.1",
    "concurrently": "^9.0.1"
