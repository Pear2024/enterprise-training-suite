    "db:deploy": "prisma migrate deploy",
    "db:reset": "prisma migrate reset --force",
    "db:push": "prisma db push",
    "seed:safe-start": "tsx scripts/seed-safe-start.ts",
    "seed:asset-questions": "tsx scripts/seed-asset-questions.ts",
    "check": "pnpm lint && pnpm typecheck",
    "lint:watch": "eslint . --watch",
    "typecheck:watch": "tsc --noEmit --watch",
    "dev:quality": "concurrently \"pnpm lint:watch\" \"pnpm typecheck:watch\""
  },
  "dependencies": {
